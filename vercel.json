{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/application/generate/index.ts": {
      "memory": 1024,
      "maxDuration": 15
    }
  },
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/" }
  ],
  "buildCommand": "pnpm build",
  "outputDirectory": "dist",
  "framework": "vite"
}
